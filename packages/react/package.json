  "scripts": {
    "build": "unbuild .",
    "stub": "unbuild . --stub",
    "export:sizes": "npx export-size . -r"
  },
  "peerDependencies": {
    "react": ">=18.3.1"
  },
  "build": {
    "external": [
      "react"
    ]
  },
  "dependencies": {
    "unhead": "workspace:*",
    "@unhead/schema": "workspace:*",
    "@unhead/shared": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0"
  }
