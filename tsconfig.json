{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "experimentalDecorators": true,
    "baseUrl": ".",
    "rootDir": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "unhead": ["packages/unhead/src"],
      "unhead/*": ["packages/unhead/src/*"],
      "@unhead/dom": ["package-aliases/dom/src"],
      "@unhead/ssr": ["package-aliases/ssr/src"],
      "@unhead/vue": ["packages/vue/src"],
      "@unhead/vue/*": ["packages/vue/src/*"],
      "@unhead/scripts": ["packages/scripts/src"],
      "@unhead/scripts/*": ["packages/scripts/*"],
      "@unhead/addons": ["packages/addons/src"],
      "@unhead/addons/*": ["packages/addons/*"]
    },
    "resolveJsonModule": true,
    "types": ["vitest/globals"],
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
