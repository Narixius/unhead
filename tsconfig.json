{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "baseUrl": ".",
    "rootDir": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "unhead": ["packages/unhead/src"],
      "unhead/*": ["packages/unhead/src/*"],
      "@unhead/dom": ["packages/dom/src"],
      "@unhead/ssr": ["packages/ssr/src"],
      "@unhead/schema-org": ["packages/schema-org/src"],
      "@unhead/schema-org/*": ["packages/schema-org/src/*"],
      "@unhead/vue": ["packages/vue/src"],
      "@unhead/vue/*": ["packages/vue/src/*"],
      "@unhead/scripts": ["packages/scripts/src"],
      "@unhead/scripts/*": ["packages/scripts/*"],
      "@unhead/addons": ["packages/addons/src"],
      "@unhead/addons/*": ["packages/addons/*"],
      "@unhead/schema": ["packages/schema/src"],
      "@unhead/schema/*": ["packages/schema/*"],
      "@unhead/shared": ["packages/shared/src"],
      "@unhead/shared/*": ["packages/shared/*"]
    },
    "resolveJsonModule": true,
    "types": ["vitest/globals"],
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
